{
  "name": "st-universe/core",
  "autoload": {
    "classmap": [
      "src/Lib",
      "src/admin"
    ],
    "psr-4": {
      "Stu\\": ["src/", "tests/"]
    }
  },
  "require": {
    "devster/ubench": "^2",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-redis": "*",
    "ext-xsl": "*",
    "php": "^7.4",
    "hassankhan/config": "^2.0",
    "mnsami/composer-custom-directory-installer": "^1.1",
    "scn/phptal": "^3.0",
    "zendframework/zend-mail": "^2",
    "php-di/php-di": "^6.0",
    "doctrine/orm": "^2.6",
    "mpscholten/request-parser": "^1.5",
    "graphp/graphviz": "^0.2.1",
    "st-universe/planet-generator": "dev-master",
    "st-universe/starsystem-generator": "dev-master",
    "jbbcode/jbbcode": "^1.3",
    "slim/slim": "^4.2",
    "tuupola/slim-jwt-auth": "^3.4",
    "slim/http": "^0.8",
    "slim/psr7": "^0.6",
    "opis/json-schema": "^1.0",
    "filp/whoops": "^2.5",
    "monolog/monolog": "^2.0",
    "hackzilla/password-generator": "^1.4",
    "cache/array-adapter": "^1.0",
    "cache/redis-adapter": "^1.0",
    "cache/psr-6-doctrine-bridge": "^3.0",
    "usox/ipintel": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "mockery/mockery": "^1.1",
    "st-universe/assets": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "config": {
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "tests": "@php vendor/bin/phpunit tests"
  },
  "extra": {
    "installer-paths": {
      "./src/assets": [
        "st-universe/assets"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:st-universe/assets.git"
    }
  ]
}
