{
  "name": "usox/stu3",
  "autoload": {
    "classmap": [
      "src/class",
      "src/Lib",
      "src/admin"
    ],
    "psr-4": {
      "Stu\\": "src/"
    }
  },
  "require": {
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-mysqli": "*",
    "ext-xsl": "*",
    "php": "^7.3",
    "hassankhan/config": "^2.0",
    "mnsami/composer-custom-directory-installer": "1.1.*",
    "scn/phptal": "^3.0",
    "zendframework/zend-mail": "^2",
    "php-di/php-di": "^6.0",
    "doctrine/orm": "^2.6",
    "mpscholten/request-parser": "^1.5",
    "graphp/graphviz": "^0.2.1",
    "usox/stu3-planet-generator": "dev-master",
    "usox/stu3-starsystem-generator": "dev-master",
    "jbbcode/jbbcode": "1.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.1",
    "usox/stu3-assets": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.15"
  },
  "config": {
    "platform": {
      "php": "7.3"
    }
  },
  "scripts": {
    "tests": "@php vendor/bin/phpunit tests"
  },
  "extra": {
    "installer-paths": {
      "./src/assets": [
        "usox/stu3-assets"
      ]
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:usox/stu3-assets.git"
    }
  ]
}
